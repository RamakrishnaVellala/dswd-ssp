<odoo>
  <data>
    <record model="ir.ui.view" id="dswd_registry_individual_list_tree_inherit">
      <field name="name">dswd_registry_individual_list_tree_inherit</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="g2p_registry_individual.view_individuals_form" />
      <field name="arch" type="xml">
        <xpath expr="//field[@name='gender']" position="after">
          <field name="occupation" string="Occupation"/>
          <field name="income" string="Income"/>
        </xpath>
      </field>
    </record>
   </data>
</odoo>